{
  "name": "@easydigitalstudio/express-server",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/**",
    "lint:watch": "esw -w src/**",
    "build": "babel src --out-dir lib",
    "test": "HTTP_PROXY= ./node_modules/.bin/_mocha --require babel-polyfill --require babel-core/register \"src/**/*.spec.js\" --timeout 10000",
    "cover": "HTTP_PROXY= ./node_modules/.bin/nyc ./node_modules/.bin/_mocha --require babel-polyfill --require babel-core/register \"src/**/*.js\"",
    "cover:view": "nyc report --reporter=lcov && open ./coverage/lcov-report/index.html",
    "pre-commit": "npm-run-all --parallel cover lint check-nsp"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.1",
    "mongodb": "^2.0.0",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.0.0",
    "swagger-ui-dist": "^3.17.3",
    "winston": "^3.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "dotenv-extended": "^2.0.1",
    "eslint": "^5.0.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "nsp": "^3.0.0",
    "nyc": "^11.2.1",
    "proxyquire": "^2.0.0",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "./eslint.js"
  },
  "pre-commit": "pre-commit"
